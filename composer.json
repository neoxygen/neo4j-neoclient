{
    "name": "neoxygen/neoclient",
    "type": "library",
    "description": "NeoClient is the most advanced Http Client for Neo4j",
    "keywords": [
        "graph",
        "neo4j",
        "cluster",
        "client",
        "high-availibility"
    ],
    "homepage": "http://neoxygen.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Christophe Willemsen",
            "email": "chris@neoxygen.io"
        }
    ],
    "require": {
        "php" : ">= 5.4",
        "symfony/console" : "~2.5",
        "symfony/yaml" : "~2.5",
        "symfony/config" : "~2.5",
        "symfony/dependency-injection" : "~2.5",
        "symfony/event-dispatcher" : "~2.5",
        "guzzlehttp/guzzle" : "^5.0",
        "monolog/monolog": "~1.1"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0",
        "phpunit/phpunit": "4.*",
        "bossa/phpspec2-expect": "*",
        "behat/behat": "~3.0"

    },
    "autoload": {
        "psr-4": {
            "Neoxygen\\NeoClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Neoxygen\\NeoClient\\Tests\\": "tests/Neoxygen/NeoClient/Tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    }
}
